  },
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.14",
    "chai": "^3.5.0",
    "connect-redis": "^3.2.0",
    "coveralls": "^2.13.1",
    "express": "^4.15.2",
    "express-session": "^1.15.2",
    "mocha": "^3.3.0",
    "nyc": "^10.3.0",
    "rimraf": "^2.6.1",
    "supertest": "^3.0.0",
    "ts-node": "^3.0.2",
    "tslint": "^5.1.0",
    "typescript": "^2.3.2"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "nyc": {
    "include": [
      "src/*.ts"
    ],
    "exclude": [
      "lib"
